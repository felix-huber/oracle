#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/markdansi@0.1.3/node_modules/markdansi/dist/node_modules:/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/markdansi@0.1.3/node_modules/markdansi/node_modules:/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/markdansi@0.1.3/node_modules:/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/markdansi@0.1.3/node_modules/markdansi/dist/node_modules:/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/markdansi@0.1.3/node_modules/markdansi/node_modules:/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/markdansi@0.1.3/node_modules:/Users/steipete/Projects/oracle-anthropic/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/markdansi@0.1.3/node_modules/markdansi/dist/cli.js" "$@"
else
  exec node  "$basedir/../.pnpm/markdansi@0.1.3/node_modules/markdansi/dist/cli.js" "$@"
fi
